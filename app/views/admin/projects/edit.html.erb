<div class="pagepad">
  <div id="breadcrumb">
    <ul>
      <li><%= link_to('Projects', admin_projects_path) %></li>
      <li class="last"><%= @project.name %></li>
    </ul>
  </div>
  
  <h1><%= @project.name %></h1>
  
  <div class="projectedit">
    
    <%= form_for @project, :url => admin_project_path(@project) do |f| %>  
      <%= render 'form', :f => f %>
    <% end %>
  
    <%= render 'leadership_sidebar' %>

  
  
  </div>
  
  <div class="projectfooter">
    <div class="clear"></div>
  </div>

</div>