<div class="pagepad">
  <h2>Students who have alternative arrive/leave dates:</h2>
  <table class="people">
    <tr>
      <th class="name">Student Name</th>
      <th>Project Name</th>
      <th>Start Date</th>
      <th>End Date</th>
      <th>Designation Number</th>
    </tr>
    <% @applications.each do |app| %>
        <tr>
          <td class="name"><%= app.name %></td>
          <td class="name"><%= app.project.name %></td>
          <td class="<%= highlight_date(app, "start_date") %>"><%= l(app.start_date) %></td>
          <td class="<%= highlight_date(app, "end_date") %>"><%= l(app.end_date) %></td>
          <td><%= app.designation_number %></td>
        </tr>
    <% end %>
  </table>
</div>
