    </ul>
    <ul>
      <li>
        <%= link_to('Total number of applicants by partner of project', total_num_applicants_by_partner_of_project_admin_reports_path) %> (<%= link_to('download', total_num_applicants_by_partner_of_project_admin_reports_path(:format => 'csv')) %>)
