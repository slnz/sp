  .sprite-discover{ background-position: 0 -199px; width: 16px; height: 11px; }
  .sprite-electronic_funds_transfer{ background-position: 0 -260px; width: 16px; height: 17px; }
  .sprite-internal_gift_transfer{ background-position: 0 -327px; width: 16px; height: 16px; }
  .sprite-mastercard{ background-position: 0 -393px; width: 16px; height: 10px; }
  .sprite-money_order{ background-position: 0 -453px; width: 16px; height: 16px; }
  .sprite-non-cash_gift{ background-position: 0 -519px; width: 16px; height: 16px; }
  .sprite-other_credit_card{ background-position: 0 -585px; width: 16px; height: 16px; }
  .sprite-personal_check{ background-position: 0 -651px; width: 16px; height: 17px; }
  .sprite-visa{ background-position: 0 -718px; width: 16px; height: 16px; }
  .sprite-wire_transfer{ background-position: 0 -784px; width: 16px; height: 16px; }
  td {
    border: 1px solid #ccc;
    padding: 2px 6px;
  }
  tr.odd {background: #f0f0f0;}
  tr.even {background: #fff; }
  th {
    background: #c0e2f6;
    text-align: left;
    border: 1px solid #ccc;
    padding: 2px 6px;
  }
  tr.totals {
    font-size: 14px;
    font-weight: bold;
    background: #fff;
    td {
      padding: 8px 6px;
      border: none;
    }
  }
  td.amount, tr:hover {
    background:#e0f2fd;
  }
}

.donationsub {
  font-size: 18px;
}
.floatleft {
  float: none;
  text-align: left !important;
}

/* Users Tables - based on People Table */

table.manageusers {
  tr:hover {
    background: #eee;
  }
  thead tr:hover {
    background: none;
  }
}



/* Modal Boxes */
.ui-dialog {
  font-family: Arial;
  -moz-box-shadow: 0px 0px 10px #555;
  .ui-dialog-content {
    padding: 0.5em 5px;
    ul {list-style: none; margin: 0; padding: 0;  height: 230px; overflow: auto; border: 1px solid silver; margin: 10px 0;}
    ul li a {color: #0b496b; padding: 4px; display: block; font-size: 12px;}
    ul li a:hover {background: #0b496b; color: #fff;}
    }
  .ui-dialog-buttonpane {
    padding: 0.3em .5em 0.15em 0.2em;
    button {
      margin: .25em .4em;
      line-height: 1em;
      padding: 0;
      }
    }
  }

.ui-dialog-titlebar {
  background: $lime;
  }

.ui-corner-all {
  -moz-border-radius: 0;
  -webkit-border-radius: 0;
  border-radius: 0;
}


.modal_person_info td {vertical-align: top; padding: 4px 0; line-height: 1.35em;}
.modal_person_info td.left {width: 70px; font-weight: bold;}

.ui-widget-content .ui-icon-person-female {background: url("<%= asset_path('icons/user-female.png') %>") no-repeat 0 5px; height: 30px;}
.ui-widget-content .ui-icon-person-male {background: url("<%= asset_path('icons/user.png') %>") no-repeat 0 5px; height: 30px;}
.ui-widget-content a {color: $dark-blue;}
.ui-widget-content a:hover {text-decoration: underline;}

a.edit_person_link {
  background:url("<%= asset_path('icons/pencil-smaller.png') %>") no-repeat scroll 0 50% transparent;
  display:block;
  font-size:12px;
  margin-top:20px;
  padding-left:23px;
}

a.AddNewPerson {
  background:url("<%= asset_path('plus.png') %>") no-repeat scroll 0 50% #E9F7FF;
  display:block;
  font-family:Arial;
  font-weight:normal;
  height:36px;
  padding-left:43px;
  padding-top:1px;
  }

/* Email Wrapper */

.emailwrap {
  background: #E0ECFF;
  border: 3px solid $dark-blue;
  padding: 10px 15px;

  .field {
   label {
    float: left;
    width: 50px;
    font-size: 12px;
    }
   input[type=text] {width: 95%; font-size: 12px;}
   input.frombox {width: 35%; float: left}
   textarea {width: 95%;}
   select {padding: 2px 2px 2px 0; font-family: Arial; font-size: 12px;}
  }
  .fieldinput {
    margin-left: 60px;
    overflow: hidden;
    width: 93%;
  }
  .addresses {
    border: 1px solid $form-gray;
    font-family: Arial;
    font-size: 12px;
    padding: 3px;
    }
}

.backbutton {
  display: block;
  margin: 0 0 15px 0;
  text-align: right;
  a {
    background: url("<%= asset_path('icons/arrow-curve-180.png') %>") no-repeat 0 50%;
    padding-left: 20px;
    }
  }

/* Person Search */
#search_applicant_wrap {
  background: $global-bg;
  border: 1px solid $form-gray;
  padding: 15px 0 0px 15px;
  div.fieldgroup {
    float: left;
    margin-right: 20px;
  }

  h4#showhide {
    margin: -15px 0 0px -15px;
    background: $dark-blue;
    color: $white;
    font-family: $global-font;
    font-size: 14px;
    padding: 10px 15px;
    a {
      color: $white;
    }
  }

  table.people td {
    text-align: left;
  }

  #allfields {
    display: block;
    padding-top: 10px;
    padding-bottom: 15px;
  }

  button {
    margin: 8px 0 0 0;
  }

  select {
    border: 1px solid $form-gray;
    padding: 3px;
  }

  select#school {
    width: 200px;
  }

  select#preference {
    width: 200px;
  }


  div.fieldinline {
    float: left;
    margin-right: 10px;
  }
}


/* Feedback */

#feedback {
  position: fixed;
  bottom: 0;
  fixed: fixed;
  background: #0b486b;
  right: 0;
  overflow: hidden;
  height: 25px;
  border: 2px solid #fff;
  border-bottom: none;
  border-right: none;
  width: 120px;
  -moz-box-shadow: 0 0 10px #000;
  -webkit-box-shadow: 0 0 10px #000;
  box-shadow: 0 0 10px #000;
  cursor: default;
}


#feedback:hover {
  width: auto;
  height: auto;
}

#feedback h2 {
  color: #fff;
  padding: 5px 10px 5px 30px;
  margin: 0;
  font-size: 13px;
  background: url("<%= asset_path('smile.png') %>") 10px 50% no-repeat;
}

#feedback p {
  margin: 0;
  padding: 0px 10px 10px 10px;
  width: 220px;
  color: #ddd;
}

#feedback a {
  color: $white;
  font-weight: bold;
  text-decoration: underline;
  cursor: pointer;
}

.deadline0 {color: red;}
.deadline1 {color: blue;}
.deadline2 {color: green;}
.deadline3 {color: black;}

.ui-helper-hidden-accessible {display:none;}

.highlight {
    background-color: #ffff00;
}

.warning {
  color: red;
}

.right_label {
  width: 60%;
  float: right;
}

#status {
  height: auto !important;
  width: auto !important;
  position: static !important;
}

.actions.unarchive {
  min-width: 90px;
}

.ui-tabs .ui-tabs-nav li a {
    font-size: 11px;
}

#evalcontent table {
    border-collapse: separate !important;
}

body#admin_projects .sidebar .module .controls {
    -moz-box-shadow: none;
    -webkit-box-shadow: none;
    box-shadow: none;
    margin: 0 auto;
    padding: 0 auto;
}