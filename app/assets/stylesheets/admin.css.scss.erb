/* Variables */
$page-width: 1000px;
$global-font: Arial, Helvetica, Verdana;
$global-bg: #EFEFEF;
$white: #fff;
$black: #000;
$form-gray: #999;
$dark-blue: #0B486B;
$dark-gray: #4D4D4D;
$lime: #CFF09E;
$deselected-global: #79B2CC;


body {margin: 0; padding: 0; font-size: 12px; font-family: $global-font; background: $global-bg; outline: none;}
a {text-decoration: none; color: $dark-blue; outline: none;}
a:active {outline: none;}
:focus {-moz-outline-style: none;}
img {border: none; vertical-align: middle;}
.pagewidth {max-width: $page-width; min-width: 960px; margin: 0 auto;}
@mixin strip-list{ margin: 0; list-style: none; padding: 0;}
.strip-list {
  @include strip-list;
}
.pagepad {padding: 15px;}
.clear {clear: both;}
.spinner {vertical-align: middle; }
.roundit {-moz-border-radius: 10px; -webkit-border-radius: 5px; border-radius: 5px;}

h1, h2, h3, h4, h5, h6 {font-family: "Gill Sans MT", Arial, Verdana, Helvetica;}
h1 {font-size: 26px;margin: 0.5em 0 1.15em 0;}
h3 {font-size: 14px;}

h3.critical {
  border-color:orange -moz-use-text-color;
  border-style:solid none;
  border-width:1px medium;
  color:orange;
  margin-top:25px;
  padding:10px 0;
}

/* Global */
#global {
  background: $white;
  border-top: 1px solid $white;
  #menu, #menuright {
    float: left;
    width: 500px;
    height: 60px;
    @include strip-list;
    li {float: left; line-height: 60px;}
    li a {
			background: $global-bg;
      color: $dark-blue;
      padding: 9px 12px;
      -webkit-border-radius: 5px;
      -moz-border-radius: 5px;
      border-radius: 5px;
      margin: 8px;
      font-size: 14px;
      font-weight: bold;
    }
    li a.selected, li a:hover {
      color: $global-bg;
      background: $dark-blue;
    }
    li.withicon {}
    li.withicon a {background: no-repeat 0px; border: 1px solid silver;}
    li.search {
      float: left;
      a {background-image: url("<%= asset_path('buttonbg_search.png') %>"); padding-left: 31px; border: none;}
      a:hover {
        color: $dark-gray;
      }
    }

  }
  #menuright {
    width: auto;
    float: right;
    li a {
      background: none;
    }
  }
  #menu {
    width: auto;
  }
}


/* Primary */

#primary {
  padding: 0 25px 0 35px;
  padding-bottom: 50px;

  .pagewidth {
      margin-top: 1.5em;
      background: $white;
      border:1px solid $dark-gray;
      border-top: 4px solid $dark-blue;
    }
  }


/* Dashboard */

.dashboardlist {
  .buttons {
    margin-left:-44px;
    margin-top:-19px;
    padding:4px;
    position:absolute;
    width:36px;
    display: none;
    padding: 0px;
    border-top: 0px;
    }
  table {
    border-collapse: collapse;
    width: 100%;
    th {
      font-size: 14px;
      text-align: left;
      text-transform: uppercase;
      }
    th.capitalize {
      text-transform: capitalize;
      }
    th.th_project {width: 40%;}
    th.th_pd {}
    th.th_opd {}
    th.th_apd {}
    th.th_count {width: 65px;}
    td {border-bottom: 1px solid $global-bg; padding: 7px 0;}
    td.close a {color: $dark-gray;}
    td.count {padding-left: 8px;}
    }

    th{
      input {
        font-size: 11px;
        }
      input:focus {
        background: $white;
        }
      input#search {
        width: 200px;
        }
      input.prompt {
        color: silver;
        }
      }

  .rollovershow {
    position: absolute;
    margin: -8px 0 0 -65px;
    background: #fff;
    padding: 8px 3px 8px 7px;
    width: 54px;
    -moz-border-radius: 5px 0 0 5px;
    display: block;
    font-size: 11px;
    border: 1px solid $global-bg;
    border-right: none;

    .edit {
      margin-left: 5px;
      }
    }
  .rollovershow_nostyle {
  }

  .rollovershow:hover {
    display: block;
    }
  .rollovershow_nostyle:hover {
    display: block;
    }
  }

/* Project Edit */

p.legend {
	font-weight: bold;
	font-size: 11px;

	img {
		vertical-align: top;
		}

	}

.projectedit .left {
  width: 71%;
  float: left;
  }

.projectedit p {
  margin-top: 0px;
  }

.projectedit .ui-tabs .ui-tabs-panel {
  padding-top: 1em;
  }

.projectedit .right {
  width: 254px;
  float: right;
  border-left: 1px solid $global-bg;
  padding-left: 13px;
  margin-left: 13px;
  padding-bottom: 60px;
  }

.projectedit {
  .checkboxes {
    max-height: 190px;
    overflow: auto;
    border: 1px solid $form-gray;
    width: 500px;
    background: url("<%= asset_path('checkbox_bg.jpg') %>") repeat-x 100% 100%;
    div {
      padding: 4px 0;
      }

    div.selected {
      background: $dark-blue;
      color: #fff;
      }
    div.selected:hover {
      color: #000;
      }
    div:hover {
      background: $lime;
      }
    label {
      display: inline;
      line-height: 1.05em;
      font-size: 12px;
      cursor: pointer;
      font-weight: normal;
      }
    input {
      vertical-align: middle;
      }
    }
  }

.fullpage .left {width: 100%;}

/* Forms */

label {
  font-size: 11px;
  font-weight: bold;
  display: block;
  padding: 4px;
  }

input[type=text] {
  border: 1px solid $form-gray;
  padding: 4px 3px;
  font-family: $global-font;
  width: 175px;
}



input[type=text].search {
	width: 100px;
}

input.small {
  width: 50px;
}

input.date {
  width: 50px;
}

input.hasDatepicker[type=text] {
  background: url("<%= asset_path('icons/calendar.png') %>") no-repeat 98% 50%;
  width: 100px;
}

div.field {
  margin-bottom: .75em;
  select {
    border: 1px solid $form-gray;
    padding: 4px 3px;
    min-width: 330px;
  }
}

div.multifield .field {
  float: left;
  margin-right: 0.5em;
}

div.required label {
	padding-left:15px;
	background: url("<%= asset_path('required.png') %>") no-repeat scroll 0 33% transparent;
}

div.quote {
  textarea {
    height: 50px;
  }
  a.deletequote {
    position: absolute;
    margin-left: 190px;
    opacity: .6;
    font-size: 11px;
    font-weight: bold;
    margin-top: 4px;
    img {
      border: none;
      vertical-align: bottom;
      margin-right: 3px;
      }
    }
  a.deletequote:hover {
    opacity: 1;
    }
  }


a.addanother {
  font-size: 11px;
  font-weight: bold;
  color: #2A7F47;
  img {
    border: none;
    vertical-align: bottom;
  }
}

div.picture {
  background: url("holder.png") no-repeat 50% 50%;
  border: 1px solid silver;
  width:100px;
  height: 100px;
  margin-right: 20px;
  vertical-align: middle;
  }

.ui-widget input.smallinputfile {
  font-size: 12px;
  }

button {
  background: url("<%= asset_path('buttonbg.png') %>") repeat-x;
  border: 1px solid silver;
  -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
  border-radius: 5px;
  font-size: 14px;
  font-weight: bold;
  margin: 8px;
  padding: 0 10px;
  line-height: 1em;
  font-family: $global-font;
  span {
    display: block;
    padding: 4px;
  }
}

button.withicon span {
  padding-left: 31px;
}

button.save span {
  padding: 9px 6px 9px 6px;
  background: url("<%= asset_path('buttonbg.png') %>") no-repeat 0px 50%;
  display: block;
}

.checkvertical .checkboxwrapper {
  display: block;
  label {
    display: inline;
    font-weight: normal;
    padding: 0;
    line-height: 1em;
    font-size: 12px;
    }
  }

/* Helper */
.tooltip {
  background: #efefef;
  border: 2px solid #fff;
  -moz-box-shadow: 0 0 5px #000;
  -webkit-box-shadow: 0 0 5px #000;
  box-shadow: 0 0 5px #000;
  padding: 8px;
}

.inlinetip {
  color: $dark-gray;
  font-size: 11px;
  display: block;
  margin-top: 0px;
  margin-bottom: 15px;
  margin-left: 0px;
  width: 594px;

  .explanation {
    height: 40px;
    padding: 11px;
    background: #FFF9DC;
    overflow: hidden;
  }

  p {
    height: 1.25em;
    overflow: hidden;
    margin-bottom: 0;
  }
  .showall {
    height: auto;
  }
  span#readmoreless {
    cursor: pointer;
    color: $dark-blue;
    display: block;
    padding: 4px 5px;
  }
}


/* Tips */
label, h3 {
  .inlinetip {
    font-family: Arial, Verdana;
    font-size: 11px;
    font-weight: normal;
    display: inline;
  }
  .inlinetip img {
    vertical-align: middle;
  }
  .inlinetip .tip {
    display: inline;
    background: #FFF9DC;
    padding:2px 5px;
    font-weight: normal;
  }
}


/* right side column */
div.module {
  h3 {
    background: $global-bg;
    padding: 6px 10px;
    margin-bottom:0.5em;
    min-height: 20px;
    }
  table {
    width: 100%;
    margin: 0 6px;
    th {
      text-align: left;
      }
    td.rightalign {
      text-align: right;
      }
    td.name {
      }
    td.icon {
      width: 25px;
      text-align: center;
      }
    td.titlefield {
      width: 90px;
      }
    div.buttons {
      float: right;
      margin-right: 7px;
      a.delete {margin-left: 4px;}
      padding: 0px;
      border-top: 0px;
      }

    }
  table.stafflist {
    margin-bottom: .75em;
    }
  }

a span.fillit {color: silver; font-style: italic;}
a:hover span.fillit {color: $dark-blue; font-style: normal;}


div.applicantdata {
  background: $lime;
  h3 {background: $dark-blue; color: $white;}
    table {
    width: 100%;
    margin-bottom: 10px;
    th.rightalign {
      width: 20%;
      padding-right: 9px;
      }
    th.icon, td.icon {
      text-align: center;
      width: 15%;
      }
    }
  }


/* iconic links */
.add {
  background: url("<%= asset_path('icons/plus-button.png') %>") no-repeat 0 50%;
  padding-left: 20px;
  display: inline-block;
  }

.module h3 span {
  float: left;
  padding-top: 3px;
  }



.module h3 span.controls {
  font-size: 11px;
  margin: 3px 0;
  display: block;
  border:0px solid red;
  float: right;
  }


/* Tabs */
.ui-widget input, .ui-widget select, .ui-widget textarea, .ui-widget button {
  font-family: $global-font;
  }
.ui-tabs {
  padding: 0;
  border: none;
  font-family: $global-font;

  .ui-tabs-nav {
    background: none;
    padding: 0;
    li {
      margin: 0 0.4em 1px 0;
      border: none;
      a {
        font-size: 12px;
        }
      }
    li.ui-tabs-selected {
      padding: 0;
      background: $dark-blue;
      a {
        color: $white;
        cursor: default;
        }
      }
    }

  .ui-corner-all {
    -moz-border-radius: 0;
    border: 0px solid $dark-blue;
    border-bottom: 3.5px solid $dark-blue;
    }

  .ui-tabs-panel {
    padding: 0;
    -moz-border-radius: 0;
    }

  }

.col_left {float: left; width: 49%;}
.col_right {float: right; width: 49%;}

hr {border: 0px; border-top: 1px solid $global-bg; margin: 1.5em 0; clear: both; }


/* Icons */
a.inline {
  background: no-repeat 50% 50%;
  width: 15px;
  text-indent: -10000px;
  overflow: hidden;
  display: inline-block;
  zoom: 1;
  *display: inline;
  line-height: auto;
  vertical-align: bottom;
}

a.edit {background-image: url("<%= asset_path('icons/pencil-smaller.png') %>");}
a.delete {background-image: url("<%= asset_path('icons/slash.png') %>");}
a.stats {background-image: url("<%= asset_path('icons/chart-pie.png') %>"); width: 16px; height: 16px;}
a.close {text-indent: 0px; width: auto;}

/* Breadcrumbs */

#breadcrumb {
  height: 2em;
  ul {
    @include strip-list;
    font-size: 11px;
    li {
      float: left;
      background: url("<%= asset_path('breadcrumbarrow.png') %>") no-repeat 100% 50%;
      padding-right: 10px;
      margin-right: 5px;
    }
    li.last {
      background: none;
      padding-right: 0;
      margin-right: 0;
      a {color: $black;}
      }
  }
}

/* Pagination */
.show_select {
  float: right;
  margin-top: 20px;
}

.pagination_wrapper {
  font-size: 14px;
  font-weight: bold;
  margin-top: 20px;
  float: left;
}

.pagination {
  display: inline-block;
  .previous_page {display: none;}
  .next_page {display: none;}
  a, em {
    border-radius: 4px;
    -moz-border-radius: 4px;
    -webkit-border-radius: 4px;
    background: $white;
    display: inline-block;
    padding: 3px 4px;
    font-size: 14px;
    }
  em {
    background: $lime;
    color: $black;
    font-weight: bold;
    padding: 3px 7px;
    font-style: normal;
    }
  a:hover {
    background: $global-bg;
    }
}


#contextmenu {
  margin-bottom: 30px;
  ul.right, ul.left {
    float: right;
    @include strip-list;
    li {float: left; line-height: 28px;}
    li {
      a {
        padding: 9px 12px;
        -webkit-border-radius: 5px;
        -moz-border-radius: 5px;
        border-radius: 5px;
        margin: 8px;
        font-size: 14px;
        font-weight: bold;
        background-color: $global-bg;
      }
      a.withicon {
        padding-left: 32px;
        background-repeat: no-repeat;
        background-position: 10px 50%;
      }
      a.person { background-image:  url("<%= asset_path('icons/user.png') %>");}
      a.add { background-image:  url("<%= asset_path('icons/plus-button.png') %>");}
      a.email { background-image:  url("<%= asset_path('icons/mail-open-document.png') %>");}
      a.excel { background-image:  url("<%= asset_path('icons/document-excel.png') %>");}
      a.down {background-image: url("<%= asset_path('icons/down.png') %>"); background-position: 0 50%; border: 1px solid silver; position: relative; z-index: 100;}
    }
  }

  .poptions {
    width: 620px;
    min-height: 200px;
    background: $global-bg;
    position: absolute;
    margin-top: 42px;
    padding: 10px;
    -moz-box-shadow: 0 0 12px #555555;
    -webkit-box-shadow: 0 0 12px #555555;
    box-shadow: 0 0 12px #555555;

    ul {
      @include strip-list;
      float: left;
    }
    ul li {
      float: none;
      line-height: 2.75em;
      a {
        padding: 6px 6px 6px 20px;
        color: $black;
      }
      a.selected, a.hover {
        color: $white;
        background: url("<%= asset_path('icons/check.png') %>") no-repeat 5px 50% $dark-blue;
      }
    }
    hr {
    	margin: 0 0 5px 0;
    	border-color: gray;
    	}
  }
}

#contextmenu ul.left {
  float: left;
  li a {margin-left: 0;}
}


/* Errors */
.flash {
	margin: 0 auto;
	margin-top: 20px;
	width: 935px;

	}

.flash, #errorExplanation {
  border: 2px solid $dark-gray;
  -moz-border-radius: 7px;
  -webkit-border-radius: 7px;
  border-radius: 7px;
  background: $global-bg;
  padding: 15px 20px 15px 45px;
  font-family: "Gill Sans MT", Verdana;
  font-weight: bold;
  margin-bottom: 1em;
  font-size: 14px;
  }

#flash_notice {
  border-color: #83CE00;
  background: url("<%= asset_path('icons/flag-green.png') %>") no-repeat scroll 15px 50% #D3F4A2;
  }

#flash_error, #errorExplanation {
  border-color: #DB4F4F;
  background: url("<%= asset_path('icons/exclamation-red.png') %>") no-repeat scroll 15px 50% #FFA4A4;
  }

#errorExplanation {
	h2 {
		font-size: 16px;
		margin: 0;
		}
	p, li {
		font-weight: normal;
		margin: 0;
		padding: 0;
		}
	ul {
		margin: 0;
		}
}

.field_with_errors {
	input {
		border-color: #DB4F4F;
	}
	label {
		color: #DB4F4F;
	}
}

.multitext .field_with_errors {
	display: inline;
	}

/* Header Drop Down */


.hybriddrop {
  a {
    padding: 9px 12px;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
    margin: 8px;
    font-size: 14px;
    font-weight: bold;
    background-color: $global-bg;
  }
  a.withicon {
    padding-left: 32px;
    background-repeat: no-repeat;
    background-position: 10px 50%;
  }
  a.person { background-image:  url("<%= asset_path('icons/user.png') %>");}
  a.add { background-image:  url("<%= asset_path('icons/plus-button.png') %>");}
  a.email { background-image:  url("<%= asset_path('icons/mail-open-document.png') %>");}
  a.excel { background-image:  url("<%= asset_path('icons/document-excel.png') %>");}
  a.down {background-image: url("<%= asset_path('icons/down.png') %>"); background-position: 0 50%; border: 1px solid silver; position: relative; z-index: 100;}

  .poptions {
    min-width: 300px;
    min-height: 200px;
    background: $global-bg;
    position: absolute;
    margin-top: 42px;
    padding: 10px;
    -moz-box-shadow: 0 0 12px #555555;
    -webkit-box-shadow: 0 0 12px #555555;
    box-shadow: 0 0 12px #555555;
    ul {
      @include strip-list;
      float: left;
    }
    ul li {
      float: none;
      line-height: 2.75em;
      a {
        padding: 6px 6px 6px 20px;
        color: $black;
      }
      a.selected, a.hover {
        color: $white;
        background: url("<%= asset_path('icons/check.png') %>") no-repeat 5px 50% $dark-blue;
      }
    }
  }
}

.hybriddropinline {
  a {
    float: left;
    margin: -10px 8px 0 0;
  }
}

.dropdowncontents {
  ul {
    -moz-border-radius:0 0 5px 5px;
    -webkit-border-radius:0 0 5px 5px;
    border-radius:0 0 5px 5px;
    -mox-box-shadow: 0px 0px 5px #999;
    -webkit-box-shadow: 0px 0px 5px #999;
    box-shadow: 0px 0px 5px #999;
    background:none repeat scroll 0 0 #EFEFEF;
    border:1px solid silver;
    display:none;
    margin-left:24px;
    margin-top:33px;
    padding:6px 0 0 0;
    position:absolute;
    z-index: 10;
  }
  ul li {
    float:none;
    list-style:none outside none;
    text-indent:0;
  }
  ul li a {
    background:#EFEFEF;
    display:block;
    float:none;
    margin:0;
    padding:3px 8px;
    width:36px;
  }
}

/* People Tables */
table.people {
  margin: 15px 0;
  width: 100%;
  border-collapse: collapse;
  th {
    text-align: left;
    font-weight: bold;
    font-size: 14px;
    }
  th, td {
    padding: 4px;
    overflow: hidden;
    }
/*  th.sort_up {background: url("sort_up.png") no-repeat 0 50%; padding-left: 12px;}
  th.sort_down {background: url("sort_down.png") no-repeat 0 50%; padding-left: 12px;}  */
  td {
    a {color: $dark-blue;}
    border-bottom: 1px solid $global-bg;
    font-size: 12px;
  }
  td.evaluate {
      text-align: center;
  }
  .gender {}
  .name {width: 23%;}
  .evaluate img {vertical-align: bottom;}
}

/* Donations Tables - based on People Table */
